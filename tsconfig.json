{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/vscode-typescript-to-lua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "types": ["typescript-to-lua/language-extensions"],
    "strict": true,
    "plugins": [
      {
        "name": "typescript-tstl-plugin"
      }
    ]
  },
  "tstl": {
    "luaTarget": "JIT"
  }
}
